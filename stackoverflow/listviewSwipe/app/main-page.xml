
<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">

    <ListView id="listView" swipe="onItemSwipe" items="{{ items }}"  loaded="onLoaded" itemLoading="onItemLoading" itemTap="onItemTap">
        <ListView.itemTemplate>
            <Label text="{{ $value }}" textWrap="true" class="m-20 p-10"/>
        </ListView.itemTemplate>
    </ListView>
</Page>
